{
  "manifest_version": 2,
  "name": "digitalCanine's Better New Tab",
  "version": "1.0.0",
  "description": "A terminal-styled new tab with search and weather",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "optional_permissions": [],
  "browser_specific_settings": {
    "gecko": {
      "id": "digitalcanines-better-new-tab@local.addon",
      "data_collection_permissions": {
        "required": [
          "none"
        ]
      }
    }
  },
  "icons": {
    "48": "./icons/icon-48.png",
    "128": "./icons/icon-128.png"
  }
}
