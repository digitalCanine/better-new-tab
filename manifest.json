{
  "manifest_version": 2,
  "name": "Better New Tab",
  "version": "1.3",
  "description": "Minimal terminal-inspired new tab page using DuckDuckGo and local history.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "optional_permissions": [],
  "browser_specific_settings": {
    "gecko": {
      "id": "better-new-tab@local.addon",
      "data_collection_permissions": {
        "required": [
          "none"
        ]
      }
    }
  }
}
